import java.util.*;

public class AutoboxingUnboxing {
    public static void main(String[] args) {
        // Creating a list of Integer objects using autoboxing
        List<Integer> numbers = new ArrayList<>();
        numbers.add(10);  // autoboxing (int to Integer)
        numbers.add(20);
        numbers.add(30);
        numbers.add(40);
        
        // Calculate the sum using unboxing (Integer to int)
        int sum = 0;
        for (Integer num : numbers) {
            sum += num;  // unboxing
        }

        System.out.println("The sum of the list of integers is: " + sum);

        // Parsing a string to Integer using Integer.parseInt
        String numberStr = "50";
        int parsedNumber = Integer.parseInt(numberStr);
        System.out.println("Parsed number: " + parsedNumber);
    }
}
